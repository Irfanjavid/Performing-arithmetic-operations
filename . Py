N1=int(input("Enter first number")) 
N2=int(input("Enter second number")) 
A=N1+N2
B=N1-N2
C=N1*N2
D=N1//N2
E=N1%N2
print("\nThe sum of two numbers is:",A,
      "\nThe difference of two numbers is:",B,
      "\nThe product of two numbers is:",C,
      "\nThe quotient of two numbers is:",D,
      "\nThe remainder is:",E,)
